{
  "manifest_version": 3,
  "name": "Task Manager",
  "version": "1.0",
  "description": "This is a simple task manager Chrome extension that allows you to create and manage tasks.",
  "permissions": ["activeTab", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}